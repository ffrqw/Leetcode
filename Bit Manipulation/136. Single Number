public class Solution {
    public int singleNumber(int[] nums) {
        // use a^a=0 && ^ is commutative
        
        // time: n
        // no space
        int num1=nums[0];
        int ans = num1;
        for(int i=1;i<nums.length;i++)
        {
            int num2 = nums[i];
            ans = num1^num2;
            num1=num2;
        }
        return ans;
    }
}
